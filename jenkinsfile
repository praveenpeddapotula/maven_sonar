     pipeline {
        agent none
        stages {
         
          stage("build & sonarqube") {
            agent any
            steps {
              withSonarQubeEnv('sonar-scan') {
                sh 'mvn clean package sonar:sonar'
              }
            }
          }
        }
      }
#withSonarQubeEnv('sonar-scan')  here sonar-scan is taken from env variable of system configuration of sonarqube
